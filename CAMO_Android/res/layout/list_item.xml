<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="wrap_content">
  <TextView android:id="@+id/textView_subject"
  	android:layout_width="match_parent"
  	android:layout_height="wrap_content"
  	android:text="subject">
  </TextView>
  <TextView android:id="@+id/textView_predicate"
  	android:layout_width="match_parent"
  	android:layout_height="wrap_content"
  	android:text="predicate">
  </TextView>
  <TextView android:id="@+id/textView_object"
  	android:layout_width="match_parent"
  	android:layout_height="wrap_content"
  	android:text="object">
  </TextView>
    
</LinearLayout>
