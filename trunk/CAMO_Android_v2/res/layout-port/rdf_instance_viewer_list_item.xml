<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android" 
  android:layout_width="match_parent"
  android:layout_height="wrap_content">  
  <TextView android:id="@+id/textView_predicate"
  	android:layout_width="match_parent"
  	android:layout_height="wrap_content"
  	android:layout_marginLeft="10dip"
  	android:text="predicate"
  	android:textColor="#EF7100">
  </TextView>
  <TextView android:id="@+id/textView_object"
  	android:layout_width="match_parent"
  	android:layout_height="wrap_content"
  	android:layout_marginLeft="20dip"
  	android:text="object"
  	android:textSize="20dip"
  	android:layout_below="@+id/textView_predicate">
  </TextView>
  <TextView android:layout_height="wrap_content" 
  android:text=">" 
  android:id="@+id/textView_accessable"
  android:textSize="40dip"
  android:layout_width="wrap_content" 
  android:layout_centerVertical="true" 
  android:layout_alignParentRight="true" 
  android:layout_marginRight="20dip" 
  android:visibility="gone"></TextView>
    
</RelativeLayout>
